{
  "_config_info": {
    "name": "Enhanced Bathymetric CAE Configuration",
    "version": "2.0",
    "description": "Default configuration for Enhanced Bathymetric Grid Processing using Advanced Ensemble CAE",
    "created_date": "2025-07-08",
    "documentation": "https://github.com/your-repo/enhanced-bathymetric-cae"
  },
  
  "_paths": {
    "description": "Input/Output path configurations"
  },
  "input_folder": "\\\\network_folder\\input_bathymetric_files",
  "output_folder": "\\\\network_folder\\output_bathymetric_files",
  "model_path": "cae_model_with_uncertainty.h5",
  
  "_logging": {
    "description": "Logging and monitoring configurations"
  },
  "log_dir": "logs/fit/20250708-120000",
  "log_level": "INFO",
  
  "_training": {
    "description": "Model training parameters"
  },
  "epochs": 100,
  "batch_size": 8,
  "validation_split": 0.2,
  "learning_rate": 0.001,
  
  "_model_architecture": {
    "description": "Neural network architecture parameters"
  },
  "grid_size": 512,
  "base_filters": 32,
  "depth": 4,
  "dropout_rate": 0.2,
  
  "_callbacks": {
    "description": "Training callback configurations"
  },
  "early_stopping_patience": 15,
  "reduce_lr_patience": 8,
  "reduce_lr_factor": 0.5,
  "min_lr": 1e-7,
  
  "_processing": {
    "description": "Data processing and file handling"
  },
  "supported_formats": [".bag", ".tif", ".tiff", ".asc", ".xyz"],
  "min_patch_size": 32,
  "max_workers": -1,
  
  "_performance": {
    "description": "Performance optimization settings"
  },
  "gpu_memory_growth": true,
  "use_mixed_precision": true,
  "prefetch_buffer_size": "AUTOTUNE",
  
  "_enhanced_features": {
    "description": "New enhanced feature toggles"
  },
  "enable_adaptive_processing": true,
  "enable_expert_review": true,
  "enable_constitutional_constraints": true,
  "quality_threshold": 0.7,
  "auto_flag_threshold": 0.5,
  "ensemble_size": 3,
  
  "_quality_metrics": {
    "description": "Multi-objective quality metric weights (must sum to 1.0)"
  },
  "ssim_weight": 0.3,
  "roughness_weight": 0.2,
  "feature_preservation_weight": 0.3,
  "consistency_weight": 0.2,
  
  "_seafloor_classification": {
    "description": "Adaptive processing parameters by seafloor type"
  },
  "depth_ranges": {
    "shallow_coastal": [0, 200],
    "continental_shelf": [200, 2000], 
    "deep_ocean": [2000, 6000],
    "abyssal_plain": [6000, 11000],
    "seamount": [0, 6000]
  },
  
  "_adaptive_strategies": {
    "description": "Processing strategy parameters for different seafloor types"
  },
  "shallow_coastal": {
    "smoothing_factor": 0.3,
    "edge_preservation": 0.8,
    "noise_threshold": 0.1,
    "gradient_constraint": 0.05,
    "feature_preservation_weight": 0.9
  },
  "deep_ocean": {
    "smoothing_factor": 0.7,
    "edge_preservation": 0.4,
    "noise_threshold": 0.2,
    "gradient_constraint": 0.1,
    "feature_preservation_weight": 0.6
  },
  "continental_shelf": {
    "smoothing_factor": 0.5,
    "edge_preservation": 0.6,
    "noise_threshold": 0.15,
    "gradient_constraint": 0.08,
    "feature_preservation_weight": 0.75
  },
  "seamount": {
    "smoothing_factor": 0.2,
    "edge_preservation": 0.9,
    "noise_threshold": 0.05,
    "gradient_constraint": 0.15,
    "feature_preservation_weight": 0.95
  },
  "abyssal_plain": {
    "smoothing_factor": 0.8,
    "edge_preservation": 0.3,
    "noise_threshold": 0.25,
    "gradient_constraint": 0.05,
    "feature_preservation_weight": 0.5
  },
  "default": {
    "smoothing_factor": 0.5,
    "edge_preservation": 0.6,
    "noise_threshold": 0.15,
    "gradient_constraint": 0.1,
    "feature_preservation_weight": 0.7
  },
  
  "_constitutional_constraints": {
    "description": "Constitutional AI constraint parameters"
  },
  "max_gradient": 0.1,
  "feature_threshold": 0.05,
  "monotonicity_direction": "increasing",
  "constraint_blend_factor": 0.5,
  
  "_expert_review": {
    "description": "Expert review system configuration"
  },
  "db_path": "expert_reviews.db",
  "flag_types": [
    "low_quality",
    "feature_loss", 
    "standards_violation",
    "poor_similarity"
  ],
  "review_thresholds": {
    "feature_preservation": 0.5,
    "hydrographic_compliance": 0.7,
    "ssim": 0.6
  },
  
  "_ensemble_configuration": {
    "description": "Ensemble model configuration"
  },
  "model_variations": [
    {
      "base_filters": 24,
      "depth": 3,
      "dropout_rate": 0.1
    },
    {
      "base_filters": 32,
      "depth": 4,
      "dropout_rate": 0.2
    },
    {
      "base_filters": 48,
      "depth": 5,
      "dropout_rate": 0.3
    }
  ],
  "equal_weights": true,
  "adaptive_weighting": false,
  
  "_quality_standards": {
    "description": "IHO S-44 and quality compliance parameters"
  },
  "iho_s44_compliance": true,
  "depth_dependent_uncertainty_formula": "0.5 + 0.013 * abs(depth)",
  "quality_categories": {
    "high": 0.8,
    "medium": 0.6,
    "low": 0.4
  },
  
  "_visualization": {
    "description": "Visualization and reporting parameters"
  },
  "generate_plots": true,
  "plot_dpi": 300,
  "color_schemes": {
    "bathymetry": "viridis",
    "difference": "RdBu_r",
    "uncertainty": "plasma"
  },
  "save_intermediate_results": false,
  
  "_optimization": {
    "description": "Memory and performance optimization"
  },
  "memory_monitoring": true,
  "garbage_collection_frequency": 5,
  "prefetch_training_data": true,
  "cache_preprocessed_data": false,
  
  "_output_formats": {
    "description": "Output format preferences"
  },
  "preserve_original_format": true,
  "default_output_format": ".tif",
  "compression": "LZW",
  "include_metadata": true,
  "metadata_domains": [
    "PROCESSING",
    "QUALITY", 
    "ADAPTIVE"
  ],
  
  "_validation": {
    "description": "Data validation parameters"
  },
  "validate_input_data": true,
  "reject_all_invalid": true,
  "outlier_detection": true,
  "outlier_percentiles": [1, 99],
  "minimum_valid_percentage": 50,
  
  "_advanced": {
    "description": "Advanced processing options"
  },
  "multi_scale_processing": false,
  "uncertainty_propagation": true,
  "edge_aware_filtering": true,
  "topology_preservation": true,
  "bathymetric_feature_detection": true
}