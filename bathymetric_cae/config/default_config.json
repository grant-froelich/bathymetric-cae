{
  "input_folder": "./input_bathymetric_files",
  "output_folder": "./output_bathymetric_files",
  "model_path": "cae_model_with_uncertainty.h5",
  "log_dir": "logs/fit",
  "log_level": "INFO",
  "epochs": 100,
  "batch_size": 8,
  "validation_split": 0.2,
  "learning_rate": 0.001,
  "grid_size": 512,
  "base_filters": 32,
  "depth": 4,
  "dropout_rate": 0.2,
  "early_stopping_patience": 15,
  "reduce_lr_patience": 8,
  "reduce_lr_factor": 0.5,
  "min_lr": 1e-7,
  "supported_formats": [".bag", ".tif", ".tiff", ".asc", ".xyz"],
  "min_patch_size": 32,
  "max_workers": -1,
  "gpu_memory_growth": true,
  "use_mixed_precision": true,
  "prefetch_buffer_size": null
}